set hidden

execute pathogen#infect()

syntax on
:set number
:set expandtab
:set softtabstop=4
:set shiftwidth=4
:set autoindent
:color darkblue
set backspace=2

filetype plugin indent on

hi Normal ctermfg=252 ctermbg=none
hi NonText ctermfg=250 ctermbg=none

set expandtab
set shiftwidth=4
set softtabstop=4
